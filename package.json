{
  "name": "first-module",
  "version": "1.0.0",
  "description": "Easy conversation",
  "scripts": {
    "start": "(npm run clear && npm run build && npm run copy) && npm run server",
    "dev": "parcel ./client/src/account.html",
    "build": "parcel build client/src/*.html",
    "html": "parcel ./client/html-templates/index.html ",
    "clear": "rm -f ./static/* ./dist/*",
    "copy": "mkdir -p ./static/ && cp ./dist/* ./static/",
    "server": "node ./server/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vicpril/middle.messenger.praktikum.yandex.git"
  },
