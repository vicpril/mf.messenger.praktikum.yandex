  "license": "ISC",
  "devDependencies": {
    "parcel-bundler": "^1.12.5",
    "sass": "^1.32.8"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.3",
    "express": "^4.17.1"
  },
  "bugs": {
    "url": "https://github.com/vicpril/middle.messenger.praktikum.yandex/issues"
  },
  "homepage": "https://github.com/vicpril/middle.messenger.praktikum.yandex#readme"
